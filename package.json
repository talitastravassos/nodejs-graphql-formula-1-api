{
  "name": "boilerplate-node-express-graphql-with-typescript",
  "version": "1.0.0",
  "description": "a boilerplate for node-express projects with GraphQL and Typescript",
  "main": "server.ts",
  "author": "talitastravassos <talitatravassos.tt@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.18",
    "npm": ">=8.18"
  },
  "scripts": {
    "build": "tsc",
    "ts.check": "tsc --project tsconfig.json",
    "start": "node dist/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --no-notify src/server.ts",
    "test": "jest --coverage"
  },
  "_moduleAliases": {
    "@services": "src/services",
    "@models": "src/models",
    "@resolvers": "src/resolvers",
    "@types": "src/types"
  },
  "devDependencies": {
    "@types/express": "^4.17.19",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "apollo-server-express": "^3.12.1",
    "apollo-server-testing": "^2.25.3",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.7.0",
    "module-alias": "^2.2.3",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "*"
  },
  "dependencies": {
    "apollo-server": "^3.12.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^15.3.0",
    "graphql-scalars": "^1.22.4",
    "node-fetch": "^3.3.2",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  }
}
