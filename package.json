{
  "name": "boilerplate-node-express-graphql-with-typescript",
  "version": "1.0.0",
  "description": "a boilerplate for node-express projects with GraphQL and Typescript",
  "main": "server.ts",
  "author": "talitastravassos <talitatravassos.tt@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.18",
    "npm": ">=8.18"
  },
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored && vercel --prod",
    "start": "node dist/server.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --no-notify src/server.ts",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.22.17",
    "@babel/node": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/types": "^7.21.2",
    "@types/babel__template": "^7.4.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "apollo-server-express": "^3.12.1",
    "apollo-server-testing": "^2.25.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "jest": "^29.6.2",
    "prettier": "^2.8.4",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "*"
  },
  "dependencies": {
    "apollo-server": "^3.12.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^15.3.0",
    "graphql-scalars": "^1.22.2",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  }
}
